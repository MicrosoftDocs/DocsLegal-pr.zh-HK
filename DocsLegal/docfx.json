{
  "build": {
    "markdownEngineName": "markdig",
    "content": [
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "exclude": [
          "**/obj/**",
          "DocsLegal/**",
          "**/includes/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg"
        ],
        "exclude": [
          "**/obj/**",
          "DocsLegal/**",
          "**/includes/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {},
    "breadcrumb_path": "/legal/bread/toc.json",
    "fileMetadata": {
      "uhfHeaderId": {
        "cognitive-services/**/*.md": "azure",
        "cognitive-services/**/*.yml": "azure",
        "sentinel/**/*.md": "azure",
        "sentinel/**/*.yml": "azure"
      }
    },
    "template": [],
    "dest": "DocsLegal",
    "brand": "dynamics"
  }
}