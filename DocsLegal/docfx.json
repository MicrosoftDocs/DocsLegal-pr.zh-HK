          "**/obj/**",
          "DocsLegal/**",
          "**/includes/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
       "ms.prod": "legal"
    },
    "fileMetadata": {},
    "template": [],
    "dest": "DocsLegal",
    "brand": "dynamics"
  }
}
