      {
        "files": [
          "**/*.md"
        ],
        "exclude": [
          "**/obj/**",
          "DocsLegal/**",
          "**/includes/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg"
        ],
